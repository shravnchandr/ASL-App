[run]
source = .
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */node_modules/*
    */venv/*
    */.venv/*
    */dist/*
    */python_code/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
